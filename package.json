{
  "name": "testapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "init:dev": "powershell -File ./src/scripts/init-dev.ps1",
    "test:integration": "node ./src/tests/integration.test.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "@aws-sdk/client-s3": "^3.312.0",
    "@aws-sdk/s3-request-presigner": "^3.312.0",
    "helmet": "^6.0.1",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/cors": "^2.8.13"
  }
}
